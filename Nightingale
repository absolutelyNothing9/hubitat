            body: '{"datapoints": {"datapoint": [{"property_name": "cmd", "value": "{\\"soundStatus\\":\\"1\\"}"}]}}'
                  ]
    
            asynchttpPost('allPurposeFlour', postParams)
            sendEvent(name: "switch", value: "on")


}
////Power Options
void setPower(newPower) {

	switch (newPower) {
	case "On":
        def postParams = [
        uri: "https://ads-field.aylanetworks.com/apiv1/groups/${groupKey}/datapoints",
		    requestContentType: 'application/json',
		    contentType: 'application/json',
            headers: ["Authorization": "Bearer ${state.accessToken}","User-Agent": "Nightingale/1.4 (iPhone; iOS 14.4; Scale/3.00)"],
            body: '{"datapoints": {"datapoint": [{"property_name": "cmd", "value": "{\\"soundStatus\\":\\"1\\"}"}]}}'
                  ]
    
            asynchttpPost('allPurposeFlour', postParams)
            sendEvent(name: "switch", value: "on")
    break ;
	case "Off":
        def postParams = [
        uri: "https://ads-field.aylanetworks.com/apiv1/groups/${groupKey}/datapoints",
		    requestContentType: 'application/json',
		    contentType: 'application/json',
            headers: ["Authorization": "Bearer ${state.accessToken}","User-Agent": "Nightingale/1.4 (iPhone; iOS 14.4; Scale/3.00)"],
            body: '{"datapoints": {"datapoint": [{"property_name": "cmd", "value": "{\\"soundStatus\\":\\"0\\"}"}]}}'
                  ]
    
        asynchttpPost('allPurposeFlour', postParams)
        sendEvent(name: "switch", value: "off")
	break ;
	}

}
////Set Sound Mode
void setSoundMode(newMode) {

	switch (newMode) {
	case "Sleep":
        def postParams = [
        uri: "https://ads-field.aylanetworks.com/apiv1/groups/${groupKey}/datapoints",
		    requestContentType: 'application/json',
		    contentType: 'application/json',
            headers: ["Authorization": "Bearer ${state.accessToken}","User-Agent": "Nightingale/1.4 (iPhone; iOS 14.4; Scale/3.00)"],
            body: '{"datapoints": {"datapoint": [{"property_name": "cmd", "value": "{\\"soundMode\\":\\"0\\"}"}]}}'
                  ]
    
            asynchttpPost('allPurposeFlour', postParams)
            sendEvent(name: "switch", value: "on")
    break ;
	case "Relax":
        def postParams = [
        uri: "https://ads-field.aylanetworks.com/apiv1/groups/${groupKey}/datapoints",
		    requestContentType: 'application/json',
		    contentType: 'application/json',
            headers: ["Authorization": "Bearer ${state.accessToken}","User-Agent": "Nightingale/1.4 (iPhone; iOS 14.4; Scale/3.00)"],
            body: '{"datapoints": {"datapoint": [{"property_name": "cmd", "value": "{\\"soundMode\\":\\"1\\"}"}]}}'
                  ]
    
        asynchttpPost('allPurposeFlour', postParams)
        sendEvent(name: "switch", value: "off")
	break ;
	}

}
